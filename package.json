{
  "name": "ai-manus-unified",
  "version": "1.0.0",
  "description": "Unified AI Agent Enterprise Platform - Merging ai-manus and syria-ai-manus",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t ai-manus-unified .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ai-manus-unified",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.52",
    "@ai-sdk/anthropic": "^3.0.39",
    "@ai-sdk/cohere": "^3.0.19",
    "@ai-sdk/deepseek": "^2.0.18",
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/xai": "^3.0.49",
    "@hono/zod-validator": "^0.7.4",
    "@octokit/rest": "^22.0.1",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "@supabase/supabase-js": "^2.75.0",
    "ai": "^6.0.78",
    "hono": "^4.9.10",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^4.104.0",
    "stripe": "^17.0.0",
    "zod": "^3.25.76",
    "dotenv": "^16.4.5",
    "playwright": "^1.42.0",
    "dockerode": "^4.0.2",
    "ioredis": "^5.4.1",
    "mongoose": "^8.9.0",
    "@huggingface/inference": "^2.6.4",
    "eventsource-parser": "^2.0.1",
    "ws": "^8.18.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.25.1",
    "@types/dockerode": "^3.3.31",
    "@types/node": "^22.13.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typescript": "^5.9.3",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "openai",
    "anthropic",
    "orchestration",
    "automation"
  ],
  "author": "Unified AI Manus Team",
  "license": "MIT"
}