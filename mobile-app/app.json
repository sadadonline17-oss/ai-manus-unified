{
  "expo": {
    "name": "AI Manus Syria",
    "slug": "ai-manus-syria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B2F24"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aimanus.syria"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B2F24"
      },
      "package": "com.aimanus.syria",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AI Manus to access your camera for document scanning."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ai-manus-syria"
      },
      "theme": {
        "name": "Syrian Sovereign Emerald â€“ New Identity Edition",
        "country": "Syria",
        "colors": {
          "primary": "#0F3D2E",
          "primaryDark": "#0B2F24",
          "secondary": "#1C5C45",
          "accentGold": "#C9A646",
          "accentSoftGold": "#E5C878",
          "background": "#071F18"
        }
      }
    },
    "owner": "ai-manus-syria"
  }
}