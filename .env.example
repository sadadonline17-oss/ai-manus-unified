# =============================================================================
# AI Manus Unified - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# =============================================================================

# =============================================================================
# Application Settings
# =============================================================================
NODE_ENV=development
LOG_LEVEL=INFO
PORT=8000

# =============================================================================
# AI Provider API Keys
# =============================================================================

# OpenAI
OPENAI_API_KEY=sk-...

# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Google AI
GOOGLE_GENERATIVE_AI_API_KEY=AIza...

# DeepSeek
DEEPSEEK_API_KEY=sk-...

# Groq
GROQ_API_KEY=gsk_...

# Mistral
MISTRAL_API_KEY=...

# xAI
XAI_API_KEY=xai-...

# Cohere
COHERE_API_KEY=...

# OpenRouter
OPEN_ROUTER_API_KEY=sk-or-...

# Together AI
TOGETHER_API_KEY=...

# Perplexity
PERPLEXITY_API_KEY=pplx-...

# HuggingFace
HUGGINGFACE_API_KEY=hf_...

# Moonshot (Kimi)
MOONSHOT_API_KEY=sk-...

# Hyperbolic
HYPERBOLIC_API_KEY=...

# GitHub Models
GITHUB_TOKEN=ghp_...

# =============================================================================
# Local AI Providers
# =============================================================================

# Ollama
OLLAMA_API_BASE_URL=http://127.0.0.1:11434

# LMStudio
LMSTUDIO_API_BASE_URL=http://127.0.0.1:1234

# =============================================================================
# Amazon Bedrock
# =============================================================================
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1

# =============================================================================
# Database Configuration
# =============================================================================

# MongoDB
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=manus
MONGODB_USER=admin
MONGODB_PASSWORD=secret

# Redis
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# PostgreSQL (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/manus

# =============================================================================
# Sandbox Configuration
# =============================================================================
SANDBOX_IMAGE=simpleyyt/manus-sandbox
SANDBOX_NAME_PREFIX=sandbox
SANDBOX_TTL_MINUTES=30
SANDBOX_NETWORK=manus-network

# =============================================================================
# Authentication
# =============================================================================
AUTH_PROVIDER=password
JWT_SECRET_KEY=your-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Password Auth
PASSWORD_SALT=
PASSWORD_HASH_ROUNDS=10

# Local Auth (for development)
LOCAL_AUTH_EMAIL=admin@example.com
LOCAL_AUTH_PASSWORD=admin

# =============================================================================
# External Integrations
# =============================================================================

# Telegram Bot
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Discord
DISCORD_BOT_TOKEN=MTEwNTc...

# Slack
SLACK_BOT_TOKEN=xoxb-...

# Stripe
STRIPE_SECRET_KEY=sk_test_...

# =============================================================================
# Deployment Platforms
# =============================================================================

# Vercel
VERCEL_TOKEN=...
VERCEL_PROJECT_ID=...

# Netlify
NETLIFY_API_KEY=...

# Cloudflare
CLOUDFLARE_API_TOKEN=...
CLOUDFLARE_ACCOUNT_ID=...

# Firebase
FIREBASE_PROJECT_ID=...

# =============================================================================
# n8n Integration
# =============================================================================
N8N_WEBHOOK_URL=http://localhost:5678
N8N_API_KEY=...

# =============================================================================
# Workflow Settings
# =============================================================================
MAX_WORKFLOW_NODES=100
MAX_PARALLEL_EXECUTIONS=5
DEFAULT_TIMEOUT=300
WORKFLOW_STORAGE_PATH=/app/data/workflows