# ============================================================================
# AI Manus Unified - Environment Configuration
# Complete environment template for the AI Agent Enterprise Platform
# ============================================================================

# ===================
# Application
# ===================
NODE_ENV=development
PORT=3000
HOST=0.0.0.0
LOG_LEVEL=info

# ===================
# Database - MongoDB
# ===================
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=manus
#MONGODB_USERNAME=
#MONGODB_PASSWORD=

# ===================
# Database - Supabase (Optional)
# ===================
#SUPABASE_URL=https://your-project.supabase.co
#SUPABASE_ANON_KEY=your-anon-key
#SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ===================
# Database - PostgreSQL (Optional)
# ===================
#DATABASE_URL=postgresql://user:password@localhost:5432/manus

# ===================
# Redis
# ===================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
#REDIS_PASSWORD=

# ===================
# Sandbox Configuration
# ===================
SANDBOX_IMAGE=ai-manus-sandbox
SANDBOX_NAME_PREFIX=sandbox
SANDBOX_TTL_MINUTES=30
SANDBOX_NETWORK=manus-network
#SANDBOX_ADDRESS=
#SANDBOX_CHROME_ARGS=
#SANDBOX_HTTPS_PROXY=
#SANDBOX_HTTP_PROXY=
#SANDBOX_NO_PROXY=

# ===================
# Search Engine
# ===================
SEARCH_PROVIDER=bing
#GOOGLE_SEARCH_API_KEY=
#GOOGLE_SEARCH_ENGINE_ID=
#BING_SEARCH_API_KEY=

# ===================
# Authentication
# ===================
AUTH_PROVIDER=local
PASSWORD_SALT=
PASSWORD_HASH_ROUNDS=10
LOCAL_AUTH_EMAIL=admin@example.com
LOCAL_AUTH_PASSWORD=admin

# ===================
# JWT Configuration
# ===================
JWT_SECRET_KEY=your-secret-key-here-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# ===================
# Orchestrator
# ===================
ORCHESTRATOR_DEFAULT_PROVIDER=openai
ORCHESTRATOR_FALLBACK_PROVIDERS=anthropic,deepseek
ORCHESTRATOR_ROUTING_STRATEGY=least-latency
ORCHESTRATOR_HEALTH_CHECK_INTERVAL=60000
ORCHESTRATOR_MAX_RETRIES=3
ORCHESTRATOR_RETRY_BACKOFF_MS=1000
ORCHESTRATOR_TIMEOUT=30000

